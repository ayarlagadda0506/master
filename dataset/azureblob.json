{
	"name": "azureblob",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"folder": {
				"type": "string",
				"defaultValue": "anil"
			},
			"file": {
				"type": "string",
				"defaultValue": "sample"
			},
			"container": {
				"type": "string",
				"defaultValue": "edis-sandbox"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().file",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().container",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": [
			{
				"name": "clnum",
				"type": "UTF8"
			},
			{
				"name": "clname1",
				"type": "UTF8"
			}
		]
	}
}